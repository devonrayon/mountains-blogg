						<script data-cfasync="false" type="text/javascript">
					function onDomReady() {
  var cb = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : noop;
  var win = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : window.top;

  if (typeof cb === 'function') {
    if (win.document.readyState !== 'loading') {
      cb();
    } else if (win.document.addEventListener) {
      win.document.addEventListener('DOMContentLoaded', cb);
    } else {
      win.attachEvent('onload', cb);
    }
  }
}

function isInIterable(node, iterable) {
  for (var i = 0; i < iterable.length; i++) {
    var currentIterable = iterable[i];
    if (currentIterable === node || currentIterable.contains(node)) {
      return true;
    }
  }

  return false;
}

function addQuotesToTestimonials() {
  var testimonialContent = jQuery('.hmi-quote .elementor-testimonial-content');
  var quote = '<img class="hmi-quote-img" src="/wp-content/uploads/2019/09/Quotation-Marks.png" />';

  testimonialContent.prepend(quote);
  testimonialContent.append(quote);
}

function closeAccordionsByDefault() {
  var accordionItems = document.querySelectorAll('.elementor-accordion-item:first-of-type');
  var openAccordions = document.querySelectorAll('.accordion-open');
  for (var i = 0; i < accordionItems.length; i++) {
    var currentItem = accordionItems[i].firstElementChild;
    if (!isInIterable(currentItem, openAccordions)) {
      currentItem.classList.remove('elementor-active');
      currentItem.nextElementSibling.classList.remove('elementor-active');
      currentItem.nextElementSibling.style = 'display: none;';
    }
  }
}

function addCloseButtonToBanner() {
  var isHidden = true;
  try {
    isHidden = Boolean(sessionStorage.getItem('hide_banner') || false);
  } catch {}
  var banner = window.document.getElementById('simple-banner');
  var closeButton = window.document.createElement('button');
  closeButton.classList.add('banner_close');
  if (isHidden) {
    banner.style.display = 'none';
  }
  closeButton.innerText = '×';
  closeButton.setAttribute('aria-label', 'Close Banner');
  closeButton.addEventListener('click', function close() {
    banner.classList.add('banner_hidden');
    try {
      sessionStorage.setItem('hide_banner', 'true');
    } catch {}
  });
  banner.appendChild(closeButton);
}

function listenForFormSubmission() {
  var actions = {
    // Form on /contact
    726: () => window.ga('send', 'event', 'form', 'submit', 'contact_us'),
  };
  window.document.addEventListener(
    'wpcf7submit',
    function (event) {
      if (typeof actions[event.detail.contactFormId] === 'function') {
        actions[event.detail.contactFormId](event);
      }
    },
    false,
  );
}

(function checkLocale() {
  var query = window.location.search.slice(1);
  var queryObject = query.split('&').reduce(function (obj, queryPart) {
    var querySplit = queryPart.split('=');
    var key = querySplit[0];
    var value = querySplit[1];
    if (key && value) {
      var toReturn = {};
      toReturn[key] = value;

      return Object.assign({}, obj, toReturn);
    }

    return obj;
  }, {});

  if (queryObject.locale) {
    document.cookie =
      'googtrans=/en/' + queryObject.locale + '; domain=' + window.location.host.split('www')[1];
    document.cookie = 'googtrans=/en/' + queryObject.locale + ';';
    window.location.replace(window.location.url.split('?')[0]);
  }
})();

(function setBlogHeader() {
  var blogTitle = document.querySelector('body.single.single-post h2');

  if (blogTitle) {
    blogTitle.textContent = 'Blog';
  }
})();

function seasonalFooterImage() {
  var seasonImages = [
    '/wp-content/uploads/2020/09/Winter-Exped.jpg',
    '/wp-content/uploads/2020/09/Spring-Canyons.jpg',
    '/wp-content/uploads/2020/09/Summer-Backpacking.jpg',
    '/wp-content/uploads/2020/09/Fall-Foliage.jpg',
  ];

  var month = new Date().getMonth();

  var season = Math.floor(month / 3);

  var imageContainer = document.querySelector(
    '.footer-bottom .elementor-inner > .elementor-section-wrap > .elementor-element',
  );

  if (imageContainer) {
    imageContainer.style.backgroundImage = 'url(' + seasonImages[season] + ')';
  }
}

onDomReady(function () {
  addQuotesToTestimonials();
  closeAccordionsByDefault();
  addCloseButtonToBanner();
  listenForFormSubmission();
  seasonalFooterImage();
});
